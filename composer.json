{
	"name": "anavaro/pmsearch",
	"type": "phpbb-extension",
	"description": "Adds functionality to search through private messages in phpBB via the UCP.",
	"homepage": "https://www.phpbb.com/community/viewtopic.php?f=456&t=2263011",
	"version": "3.0.0",
	"time": "2025-08-17",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "Leinad4Mind",
			"homepage": "https://leinad4mind.top/forum",
			"role": "Developer"
		},
		{
			"name": "Stanislav Atanasov",
			"homepage": "https://anavaro.com",
			"role": "Original Developer"
		}
	],
	"require": {
		"php": ">=8.0",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Advanced PM Search",
		"version-check": {
			"host": "lab.anavaro.com",
			"directory": "/phpbb-ext",
			"filename": "pmsearch.json"
		},
		"soft-require": {
			"phpbb/phpbb": ">=3.2,<3.3@dev"
		}
	}
}
